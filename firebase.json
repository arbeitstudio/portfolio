{
	"hosting": [
		{
			"target": "arbeitlanding",
			"public": "public",
			"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
			"cleanUrls": "true",
			"redirects": [
				{
					"source": "/discord",
					"destination": "https://discord.gg/h7rebmPu",
					"type": 301
				}
			]
		},
		{
			"target": "arbeitgit",
			"redirects": [
				{
					"source": "/",
					"destination": "https://github.com/arbeitstudio",
					"type": 301
				},
				{
					"source": "/:repo*",
					"destination": "https://github.com/arbeitstudio/:repo",
					"type": 301
				}
			]
		}
	]
}
