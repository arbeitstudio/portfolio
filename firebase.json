{
	"hosting": [
    {
      "target": "arbeitlanding",
			"public": "public",
			"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
			"cleanUrls": "true"
		},
		{
      "target": "arbeitgit",
			"redirects": [
				{
					"source": "/:repo*",
					"destination": "https://github.com/arbeitstudio/:repo",
					"type": 301
				}
			]
		}
  ]
}
